{
  "expo": {
    "name": "misCitasApp",
    "slug": "miscitasapp",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "A mobile dating app using React Native with Expo",
    "githubUrl": "https://github.com/tribiec/misCitasApp",
    "extra": {
      "API_ENDPOINT": "http://192.168.1.9:5000"
    }
  }
}
